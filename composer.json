{
    "name": "onemustcode/api",
    "description": "A simple to use api framework.",
    "type": "project",
    "licence": "MIT",
    "authors": [
        {
            "name": "Jeffrey Buitenhuis",
            "email": "jeffrey@onemustcode.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "onemustcode/api-framework": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
